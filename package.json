{
  "name": "gamegoo-v2",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "rsbuild build",
    "dev": "rsbuild dev --open",
    "preview": "rsbuild preview",
    "openapi": "openapi-generator-cli generate -i https://api.gamegoo.co.kr/v3/api-docs -g typescript-axios -o ./src/shared/api/@generated --skip-validate-spec -c openapitools.json",
    "format": "biome check --write .",
    "format:check": "biome check .",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "ci": "biome ci .",
    "postinstall": "git config core.hooksPath .githooks"
  },
  "dependencies": {
